  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^4.3.0",
    "create-react-class": "^15.6.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
