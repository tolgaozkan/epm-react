    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "transform": {
      "^.+\\.(jsx|js)?$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/src/**/*.{jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "index.jsx"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  }
